<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card card-primary card-outline card-tabs">
                    <div class="card-header p-0 pt-1 border-bottom-0">
                        <ul class="nav nav-tabs" id="custom-tabs-two-tab" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="custom-tabs-two-profile-tab" data-toggle="pill" href="#custom-tabs-two-profile" role="tab" aria-controls="custom-tabs-two-profile" aria-selected="true">Profile</a>
                            </li>
                            <!-- <li class="nav-item">
                                <a class="nav-link" id="custom-tabs-two-destinations-tab" data-toggle="pill" href="#custom-tabs-two-destinations" role="tab" aria-controls="custom-tabs-two-destinations" aria-selected="false">Destinations</a>
                            </li> -->
                        </ul>
                    </div>
                    <div class="card-body">
                        <div class="tab-content" id="custom-tabs-two-tabContent">
                            <div class="tab-pane fade show active" id="custom-tabs-two-profile" role="tabpanel" aria-labelledby="custom-tabs-two-profile-tab">
                                <form id="form" enctype="multipart/form-data" method="post" action="/admin/user-manager/update" novalidate>
                                    <input type="hidden" name="_id" value="<%- response._id %>">
                                    <div class="row">
                                        <div class="col-sm-6 form-group">
                                            <label for="username">Reporter : <%- response.displayName %></label>
                                            <!-- <input disabled type="text" id="displayName" name="displayName" placeholder="name" value="<%- response.displayName %>" class="form-control"> -->
                                        </div>
                                        <div class="col-sm-6 form-group">
                                            <label for="nftId">NFT Id : <%- response.nftId %></label>
                                            <!-- <input disabled type="text" id="nftId" name="nftId" placeholder="Full name" value="<%- response.nftId %>" class="form-control"> -->
                                        </div>                                       
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6 form-group">
                                            <label for="reason">reason</label>
                                            <input type="text" id="reason" name="reason" placeholder="Reason" value="<%- response.reason %>" class="form-control" disabled>
                                        </div>
                                        <div class="col-sm-6 form-group">
                                            <label for="comments">Comments</label>
                                            <input type="text" id="comments" name="comments" placeholder="Comments" value="<%- response.comments %>" class="form-control" disabled>
                                        </div>
                                    </div>
                                    <!-- moment(dest.start_date).format('MMM DD, YYYY') -->                                                                                                      
                                    <!-- <button class="btn btn-primary btn-sm">Delete Report</button> -->
                                    <a href="/report-manager/delete/<%- response._id %>" class="js-delete-link btn btn-danger btn-xs" data-message="Are you sure you want to delete <%- response.displayName %> ?">Delete</a>
                                    <!---<button class="btn btn-primary btn-sm">Burn NFT</button>-->
                                </form>
                            </div>                           
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
    </div>
    </div>
</section>
<%- include('./crud.ejs')%>