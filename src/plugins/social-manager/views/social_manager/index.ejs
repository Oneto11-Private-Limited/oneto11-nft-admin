<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"><%- misc.title%></h1>
            </div>
            <!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                    <li class="breadcrumb-item">Social Manager</li>                    
                </ol>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-tools">
                            <form method="get" id="search" accept-charset="utf-8" action="/social-manager">
                                <div class="input-group input-group-sm">
                                    <input type="text" name="q" class="form-control" placeholder="Name" id="q" value="">
                                    <input type="hidden" name="page" id="page" value="1">
                                    <div class="input-group-append">
                                        <button type="submit" class="btn btn-default"><i class="fas fa-search"></i></button>
                                        <a href="javascript:void(0)" id="reset" title="Reset all filters" class="btn btn-default"><i class="fas fa-sync"></i></a>
                                        
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Link</th>
                                    <th>Created</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody id="table-content">

                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                    <%- include(SRC_PATH+ '/views/admin/partials/pagination.ejs',)%>
                </div>
            </div>
        </div>
    </div>
</section>
<%- include('./script.ejs')%>