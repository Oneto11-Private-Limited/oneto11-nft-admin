<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"><%- misc.title%></h1>
            </div>
            <!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                    <li class="breadcrumb-item">Deal Manager</li>                    
                </ol>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-tools">
                            
                                <div class="input-group input-group-sm">
                                    
                                    <div class="input-group-append">
                                        
                                        <a href="/deal-manager/add" class="btn btn-default"><i class="fas fa-plus"></i> Add New</a>
                                    </div>
                                </div>
                            
                        </div>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        
          <div class="table-responsive">
            <table id="deal-table" style="width:100%" class="table table-striped table-bordered">
              <thead>
                <tr>
                  <th>Token Id</th>
                  <th>Price</th>
                  <th>Start Time</th>
                  <th>End Time</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                <% for(let item of data){ %>
									<tr>
										<td><%= item.tokenId %></td>
										<td><%= convertPrice(item.dealInfo['price']/1000000000000000000) %></td>
										<td><%= timeConverter(item.dealInfo['startTime']) %></td>
										<td><%= timeConverter(item.dealInfo['endTime']) %></td>
                                        <td><button onclick="cancel('<%= item.tokenId %>')">Cancel</button></td>
									</tr>
									<% } %>
               
                
              </tbody>
            </table>
          </div>
        

                    </div>
                    <!-- /.card-body -->
                    
                </div>
            </div>
        </div>
    </div>
</section>
<%- include('./script.ejs')%>