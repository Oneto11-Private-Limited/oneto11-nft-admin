<div class="content-header">
	<div class="container-fluid">
		<div class="row mb-2">
			<div class="col-sm-6">
				<h1 class="m-0 text-dark"><%- misc.title%></h1>
			</div>
			<!-- /.col -->
			<div class="col-sm-6">
				<ol class="breadcrumb float-sm-right">
					<li class="breadcrumb-item"><a href="/">Dashboard</a></li>
					<li class="breadcrumb-item">
						NFT Histroy Manager 
					</li>
				</ol>
			</div>
			<!-- /.col -->
		</div>
		<!-- /.row -->
	</div>
	<!-- /.container-fluid -->
</div>
<section class="content">
	<div class="container-fluid">
		<div class="row">
			<div class="col-12">
				<div class="card">
					
					<!-- /.card-header -->
					<div class="card-body">
						<div class="table-responsive">
							<table
								id="auction-table"
								style="width: 100%"
								class="table table-striped table-bordered"
							>
								<thead>
									<tr>
										<th>S.no</th>
										<th>Price</th>
										<th>Date/Time</th>
										<th>txHash</th>
										<th>Address</th>
										
									</tr>
								</thead>
								<tbody>
									<% for(const [i, item] of data.entries()){ %>
									<tr>
										<td><%= i+1 %></td>
										<td><%= convertPrice(item.amount/1000000000000000000) %></td>
										<td><%= timeConverter(item.txTime) %></td>
										<td><a href='https://mumbai.polygonscan.com/tx/<%= item.txHash %>'><%= item.txHash %></a></td>
										<td><%= item.bidder %></td>
										
									</tr>
									<% } %>
								</tbody>
							</table>
						</div>
					</div>
					<!-- /.card-body -->
				</div>
			</div>
		</div>
	</div>
</section>
<%- include('./script.ejs')%>
