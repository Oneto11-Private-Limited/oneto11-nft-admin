<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"><%- misc.title%></h1>
            </div>
            <!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                    <li class="breadcrumb-item">NFT Manager</li>                    
                </ol>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-tools">
                            
                                <div class="input-group input-group-sm">
                                    
                                    <div class="input-group-append">
                                        
                                        <!--<a href="/nft-manager/add" class="btn btn-default"><i class="fas fa-plus"></i> Add New</a>-->
                                    </div>
                                </div>
                            
                        </div>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        
          <div class="table-responsive">
            <table id="nft-table" style="width:100%" class="table table-striped table-bordered">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>Description</th>
                  <th>External Url</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
              <% for (let index = 0; index < nftvalue.length; index++) {%>
                <tr>
                  <td><%=nftvalue[index].nft_id%></td>
                  
                  <td><%=nftvalue[index].ipfs.name%></td>
                  <td><%-nftvalue[index].ipfs.description%></td>
                  <td><%=nftvalue[index].ipfs.external_url%></td>
                  <td><a href="/nft-manager/view/<%=nftvalue[index].id%>" class="btn btn-info btn-xs">View</a></td>
                 
                </tr>
                  
              <% } %>
               
               
               
                
              </tbody>
            </table>
          </div>
        

                    </div>
                    <!-- /.card-body -->
                    
                </div>
            </div>
        </div>
    </div>
</section>
<%- include('./script.ejs')%>